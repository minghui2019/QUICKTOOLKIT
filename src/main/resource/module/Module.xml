<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE Modules SYSTEM '../dtd/Module.dtd'>
<Modules>
    <Module ignore="false">
        <MainModule>
            <beanId>changeLog</beanId>
            <moduleName>ChangeLog</moduleName>
            <tableName>SYS_CHANGE_LOG</tableName>
            <moduleZHName>系统更新日志</moduleZHName>
            <packageName>com.eplugger.system.changeLog</packageName>
            <superClassMap>
                <param key="entity" value="EntityImpl" />
                <param key="bo" value="BusinessBO" />
                <param key="action" value="BusinessAction" />
            </superClassMap>
            <Fields>
                <Field>
                    <fieldId>name</fieldId>
                    <fieldName>更新名称</fieldName>
                    <dataType>String</dataType>
                    <precision>300</precision>
                </Field>
                <Field>
                    <fieldId>updateTime</fieldId>
                    <fieldName>更新时间</fieldName>
                    <dataType>Date</dataType>
                </Field>
                <Field>
                    <fieldId>estimatedDuration</fieldId>
                    <fieldName>预计维护时长（分钟）</fieldName>
                    <dataType>Integer</dataType>
                </Field>
                <Field>
                    <fieldId>predictedRisks</fieldId>
                    <fieldName>预知风险</fieldName>
                    <dataType>String</dataType>
                    <precision>500</precision>
                </Field>
                <Field>
                    <fieldId>unknownRisks</fieldId>
                    <fieldName>未知风险</fieldName>
                    <dataType>String</dataType>
                    <precision>500</precision>
                </Field>
                <Field>
                    <fieldId>updateContent</fieldId>
                    <fieldName>更新内容</fieldName>
                    <dataType>String</dataType>
                    <precision>500</precision>
                </Field>
                <Field>
                    <fieldId>finished</fieldId>
                    <fieldName>是否完成</fieldName>
                    <dataType>String</dataType>
                    <categoryName>SF</categoryName>
                    <precision>4</precision>
                </Field>
                <Field>
                    <fieldId>fullServerPush</fieldId>
                    <fieldName>是否全服推送</fieldName>
                    <dataType>String</dataType>
                    <categoryName>SF</categoryName>
                    <precision>4</precision>
                </Field>
                <Field>
                    <fieldId>stopPush</fieldId>
                    <fieldName>结束推送</fieldName>
                    <dataType>String</dataType>
                    <categoryName>SF</categoryName>
                    <precision>4</precision>
                </Field>
                <Field>
                    <fieldId>registrant</fieldId>
                    <fieldName>登记人</fieldName>
                    <dataType>String</dataType>
                    <precision>50</precision>
                </Field>
                <Field>
                    <fieldId>registrationTime</fieldId>
                    <fieldName>登记时间</fieldName>
                    <dataType>Date</dataType>
                </Field>
            </Fields>
        </MainModule>
    </Module>
</Modules>