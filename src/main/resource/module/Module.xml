			        <fieldName>所属部门</fieldName>
			        <dataType>String</dataType>
			    </Field>
			    <Field onlyMeta="true">
			        <fieldId>vproject.planEndDate</fieldId>
			        <fieldName>计划完成日期</fieldName>
			        <dataType>Date</dataType>
			    </Field>
			    <Field onlyMeta="true">
			        <fieldId>vproject.projectType</fieldId>
			        <fieldName>项目类别</fieldName>
			        <dataType>String</dataType>
			    </Field>
			    <Field onlyMeta="true">
			        <fieldId>vproject.chargerName</fieldId>
			        <fieldName>项目负责人</fieldName>
			        <dataType>String</dataType>
			    </Field>
			    <Field onlyMeta="true">
			        <fieldId>vproject.chargerCode</fieldId>
			        <fieldName>负责人编号</fieldName>
			        <dataType>String</dataType>
			    </Field>
			    <Field onlyMeta="true">
			        <fieldId>vproject.projectStatusId</fieldId>
			        <fieldName>项目状态</fieldName>
			        <dataType>String</dataType>
			    </Field>
			    <Field onlyMeta="true">
			        <fieldId>vproject.feeAuthorize</fieldId>
			        <fieldName>批准经费</fieldName>
			        <dataType>Double</dataType>
			    </Field>
			</Fields>
